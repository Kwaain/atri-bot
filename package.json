{
  "name": "atri-bot",
  "version": "1.0.0",
  "description": "Just a bot for QQ group.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev:build": "tsc -w",
    "dev:run": "nodemon dist/index.js",
    "dev": "concurrently npm:dev:build npm:dev:run",
    "start": "npm:build && node dist/index.js",
    "build": "tsc",
    "clear": "rimraf dist"
  },
  "keywords": [
    "atri",
    "bot",
    "qq",
    "oicq"
  ],
  "author": "Kwaain <i@kwaain.com>",
  "license": "MIT",
  "_moduleAliases": {
    "@": "src"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "dist/*",
      "node_modules/*"
    ]
  },
  "devDependencies": {
    "@types/delegates": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-define-config": "^1.13.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "delegates": "^1.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "oicq": "^2.3.1",
    "openai": "^3.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.28.0",
    "sqlite3": "^5.1.4"
  }
